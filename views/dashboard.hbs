  background: #23b7e5;
  border: none;
  color: #fff;
  transition: ease-in-out;
}
.alert-info i {
  background: #1485a8;
  height: 100%;
  padding: 20px;
  margin-right: 5px;
}
.alert-info a {
  padding: 10px;
  color: #fff;
  margin-top: 3px;
}
.alert-info a:hover {
  text-shadow: none;
  color: #1485a8;
}

