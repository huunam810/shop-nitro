<style>

@import "https://fonts.googleapis.com/css?family=Orbitron";


body {
  padding: 0;
  margin: 0;
  font-family: "Orbitron", sans-serif;
}

$nav-dark: #131e25;
$nav-light: #1c2b36;
$default: #4c5f70;
$default-light: lighten($default, 15%);
$default-dark: darken($default, 15%);
$primary: #7266ba;
$primary-light: lighten($primary, 15%);
$primary-dark: darken($primary, 15%);
$info: #23b7e5;
$info-light: lighten($info, 15%);
$info-dark: darken($info, 15%);
$success: #2baab1;
$success-light: lighten($success, 15%);
$success-dark: darken($success, 15%);
$warning: #edbc6c;
$warning-light: lighten($warning, 15%);
$warning-dark: darken($warning, 15%);
$danger: #e36159;
$danger-light: lighten($danger, 15%);
$danger-dark: darken($danger, 15%);

@mixin aloha($color, $dark) {
  opacity: 0;
  animation: 1s pop forwards;
  animation-delay: 0.7s;

  border-bottom: none;
  border: 2px solid $color;

  & > .panel-heading {
    text-transform: uppercase;
    border-radius: 0;
    background: $color;
    color: #fff;
    border-color: $color;
  }
  & > .panel-body {
  }
  & > .panel-footer {
    text-transform: uppercase;
    border-radius: 0;
    background: $color;
    color: #fff;
    border-color: $color;
  }
}

@mixin color($color) {
  opacity: 0;
  position: absolute;
  top: 2px;
  left: 25px;
  background: $color;
  padding: 0 5px;
  font-size: 8px;
  border-radius: 15px;
  animation: 0.5s pop forwards;
}

@mixin color-outline($color, $dark) {
  background: $color;
  border: 2px solid $dark;
  color: #fff;
  transition: all 0.2s ease-in-out;

  &:hover,
  &:active,
  &:focus {
    border-color: $color;
    background: $dark;
    color: #fff;
  }
}
.boom,
.boomy {
  margin-left: -10px;
  @media (max-width: 512px) {
    margin-left: 0;
  }

  &:hover {
    color: lighten($nav-light, 40%);
  }
}

.navigatioon {
  nav {
    position: fixed;
    padding-left: 20px;
    padding-right: 40px;
    width: 100%;
    z-index: 9999;
    background: $nav-dark;
    border-radius: 0;
    box-shadow: -1px 5px 5px #000;
    .tipText-bot {
      opacity: 0;
      background-color: $nav-dark;
      box-shadow: 0px 3px 5px #000;
      color: #fff;
      padding: 5px;
      position: absolute;
      z-index: 6;
      font-size: 10px;
      top: 80%;
      left: 60%;
      margin-left: -30px;
      transition: all 0.2s ease-in-out;
    }

    li {
      display: inline-block;
      transition: all 0.2s ease-in-out;
      a {
        color: #fff;
        transition: all 0.2s ease-in-out;

        &:hover,
        &:active,
        &:focus {
          background: transparent;
          color: lighten($nav-light, 40%);

          .tipText-bot {
            opacity: 1;
          }

          .number-notif,
          .number-mail {
            color: #fff;
          }

          .notif {
            color: $danger;
          }

          .mail {
            color: $info;
          }
        }

        .number-notif {
          @include color($danger);
          animation-delay: 1.5s;
        }
        .number-mail {
          @include color($info);
          animation-delay: 1.7s;
        }
      }
    }
  }
}

.panel-default {
  @include aloha($default, $default-dark);
}
#main {
  padding-top: 80px;
  transition: all 0.5s ease-in-out;
  margin-left: 250px;

  @media (max-width: 512px) {
    margin-left: 0;
  }
}

.btn-default-outline {
  @include color-outline($default, $default-dark);
}
.btn-primary-outline {
  @include color-outline($primary, $primary-dark);
}

.btn-info-outline {
  @include color-outline($info, $info-dark);
}

.btn-success-outline {
  @include color-outline($success, $success-dark);
}

.btn-warning-outline {
  @include color-outline($warning, $warning-dark);
}

.btn-danger-outline {
  @include color-outline($danger, $danger-dark);
}

@keyframes pop {
  0% {
    opacity: 0;
    transform: scale(0);
  }
  100% {
    opacity: 1;
    transform: scale(1);
  }
}

.sidebar {
  position: fixed;
  z-index: 9998;
  left: 0;
  top: 0;
  height: 100%;
  width: 60px;
  background: $nav-dark;
  color: #fff;
  text-align: center;
  transition: all 0.5s ease-in-out;

  .fa {
    text-shadow: 0px 2px 5px darken($nav-dark, 40%);
  }
  @media (max-width: 768px) {
    width: 0;
  }
  .hidden {
    transition: all 0.5s ease-in-out;
  }
  .profile {
    font-family: "Dancing Script", cursive;
    padding: 10px;
    transition: all 0.5s ease-in-out;
    top: 14px;
    margin-top: 20px;
    margin-bottom: 20px;
    span {
      font-size: 30px;
    }

    .fa-user {
      width: 100px;
      height: 100px;
      border-radius: 50%;
      text-align: center;
      // line-height: 100px;
      vertical-align: middle;
      padding: 25px;
      background: lighten($nav-light, 20%);
    }
